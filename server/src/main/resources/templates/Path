  ### $model$
  /$model$/{$model$_id}:
    get:
      parameters:
        - name: $model$_id
          in: path
          schema:
            type: integer
      description: Get information about $model$ by $model$ id
      responses:
        200:
          description: Successful getting
          content:
            application/json:
              schema:
                \$ref: '#/components/schemas/$model$'
        404:
          description: Not found $model$ by this $model$_id
    put:
      requestBody:
        required: true
        content:
          application/json:
            schema:
              \$ref: '#/components/schemas/$model$'
      description: Change information about $model$
      responses:
        200:
          description: Successful changing
          content:
            application/json:
              schema:
                \$ref: '#/components/schemas/$model$'
    delete:
      parameters:
        - name: $model$_id
          in: path
          schema:
            type: integer
      description: Delate information about $model$ by $model$ id
      responses:
        200:
          description: Successful deletting
        404:
          description: Not found $model$ by this $model$_id
  /$model$:
    get:
      parameters:
        - name: limit
          in: query
          required: false
          schema:
            type: integer
          description: Limit for number of returnig values
        - name: offset
          in: query
          required: false
          schema:
            type: integer
          description: Offset for number of returnig values
      responses:
        200:
          description: List of $model$s
          content:
            application/json:
              schema:
                type: array
                items:
                  \$ref: '#/components/schemas/$model$'
    post:
      requestBody:
        required: true
        content:
          application/json:
            schema:
              \$ref: '#/components/schemas/$model$'
      description: Create information about $model$
      responses:
        200:
          description: Successful adding
          content:
            application/json:
              schema:
                \$ref: '#/components/schemas/$model$'